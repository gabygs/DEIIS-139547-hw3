<?xml version="1.0" encoding="UTF-8"?>

<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <name>deiis_types</name>
  <description/>
  <version>1.0</version>
  <vendor/>
  <imports>
    <import name="org.cleartk.TypeSystem"/>
  </imports>
  <types>
    <typeDescription>
      <name>edu.cmu.deiis.types.Annotation</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>casProcessorId</name>
          <description>The unique identifier of the CAS processor that added this annotation to the CAS</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>confidence</name>
          <description>The confidence level assigned to the Annotation by the CAS processor that created it.</description>
          <rangeTypeName>uima.cas.Double</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>edu.cmu.deiis.types.Token</name>
      <description/>
      <supertypeName>edu.cmu.deiis.types.Annotation</supertypeName>
    <features>
        <featureDescription>
          <name>token_type</name>
          <description>Identifier for the token. If it comes from an Answer it will contain an "A", else it will contain a "Q".</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      <featureDescription>
          <name>line_doc</name>
          <description>Identifier for the token, to which line in the document the token corresponds to.</description>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
      <featureDescription>
          <name>POS</name>
          <description>Part of Speech. Stanford Core NLP</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>lemma</name>
          <description>
</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>namedEntity</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>edu.cmu.deiis.types.NGram</name>
      <description/>
      <supertypeName>edu.cmu.deiis.types.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>elements</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>edu.cmu.deiis.types.Annotation</elementType>
        </featureDescription>
        <featureDescription>
          <name>elementType</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      <featureDescription>
          <name>line_doc</name>
          <description>Line in the document to which the NGram corresponds to.</description>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        </features>
    </typeDescription>
    <typeDescription>
      <name>edu.cmu.deiis.types.Answer</name>
      <description/>
      <supertypeName>edu.cmu.deiis.types.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>isCorrect</name>
          <description/>
          <rangeTypeName>uima.cas.Boolean</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>edu.cmu.deiis.types.Question</name>
      <description/>
      <supertypeName>edu.cmu.deiis.types.Annotation</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>edu.cmu.deiis.types.AnswerScore</name>
      <description/>
      <supertypeName>edu.cmu.deiis.types.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>score</name>
          <description/>
          <rangeTypeName>uima.cas.Double</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>answer</name>
          <description/>
          <rangeTypeName>edu.cmu.deiis.types.Answer</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
  <typeDescription>
      <name>edu.cmu.deiis.types.Precision</name>
      <description/>
      <supertypeName>edu.cmu.deiis.types.Annotation</supertypeName>
    <features>
        <featureDescription>
          <name>precision</name>
          <description>Saves precision value.</description>
          <rangeTypeName>uima.cas.Double</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
  <typeDescription>
      <name>edu.cmu.deiis.types.NamedEntity</name>
      <description>Used for Stanford Core NLP. Named Entities.</description>
      <supertypeName>edu.cmu.deiis.types.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>entityType</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>edu.cmu.deiis.types.Sentence</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
    </typeDescription>
  </types>
</typeSystemDescription>
